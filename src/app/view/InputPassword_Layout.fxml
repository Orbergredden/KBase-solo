<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="170.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.InputPassword_Controller">
   <children>
      <Label fx:id="label_Msg" layoutX="22.0" layoutY="24.0" text="Введите пароль" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" />
      <PasswordField fx:id="passwordField_Pwd" layoutX="20.0" layoutY="55.0" promptText="пароль" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" />
      <Button fx:id="button_Ok" defaultButton="true" layoutX="20.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleButtonOk" prefHeight="25.0" prefWidth="123.0" text="Ок" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="112.0" />
      <Button fx:id="button_Cancel" layoutX="160.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleButtonCancel" prefHeight="25.0" prefWidth="123.0" text="Отмена" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="112.0" />
   </children>
</AnchorPane>
